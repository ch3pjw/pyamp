appearance:
    progress_bar: '[ -=]'
key_bindings:
    play_pause: 'space'
    volume_up:
        - '='
        - '+'
    volume_down: '-'
    seek_forward:
        - '.'
        - '>'
    seek_backward:
        - ','
        - '<'
    next_track: ']'
    previous_track: '['
persistent:
    volume: 1
    play_mode: 'album_shuffle'
library:
    database_path: '~/.pyamp/tracks.db'
    index_paths: '~/Music'
system:
    log_file: 'pyamp.log'
    log_level: 'DEBUG'
    GST_DEBUG:
    GST_DEBUG_FILE: 'gstreamer.log'
